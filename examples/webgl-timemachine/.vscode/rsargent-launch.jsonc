            "clearConsoleOnReload": true,
            "suggestPathMappingWizard": true,
            "pathMappings": [
                {
                    "url": "https://rsargent.earthtime.org/timemachine/js/org/gigapan/util.js",
                    "path": "rsargent.earthtime.org/timemachine/js/org/gigapan/util.js"
                },
                {
                    "url": "https://rsargent.earthtime.org/stories/index.ts",
                    "path": "${workspaceFolder}/index.ts"
                },
                {
                    "url": "https://rsargent.earthtime.org/stories/index.ts",
                    "path": "${workspaceFolder}/index.ts"
                }
            ]
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "https://rsargent.earthtime.org/stories/testrs",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "https://rsargent.earthtime.org/stories/testrs",
